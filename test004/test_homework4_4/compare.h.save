#ifndef COMPARE_H_INCLUDED
#define COMPARE_H_INCLUDED
template<class numtype>
class Compare
 {public:
       Compare(numtype a,numtype b);
   numtype max();
   numtype min()
    {return (x<y)?x:y;}
      private:
        numtype x,y;
 };
 
Compare::Compare(numtype a,numtype b)
    {x=a;y=b;}
numtype Compare::max()
    {return (x>y)?x:y;}


#endif // COMPARE_H_INCLUDED
